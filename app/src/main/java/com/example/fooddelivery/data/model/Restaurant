import kotlinx.serialization.Serializable

@Serializable
data class restaurant(
    val id_restaurant: String,
    val name: String,
    val delivery_price: String?,
    val logo: String?,
    val location: String,
    val type: String,
    val rating: Double,
    val nbr_reviews: Int,
    val phone: String?,
    val email: String?,
    val instagram: String?,
    val facebook: String?
)